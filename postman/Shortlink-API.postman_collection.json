        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"testuser\",\n  \"password\": \"password\",\n  \"rememberMe\": true\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/shortlink/admin/v1/user/login",
              "host": ["{{base_url}}"],
              "path": ["api", "shortlink", "admin", "v1", "user", "login"]
            }
          },
          "response": []
